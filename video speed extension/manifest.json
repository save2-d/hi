{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "linux": "Ctrl+Period",
            "mac": "Alt+Y",
            "windows": "Ctrl+Period"
         }
      },
      "left": {
         "description": "Decrease Speed",
         "suggested_key": {
            "default": "Ctrl+Down",
            "mac": "Command+Shift+Left"
         }
      },
      "right": {
         "description": "Increase Speed",
         "suggested_key": {
            "default": "Ctrl+Up",
            "mac": "Command+Shift+Right"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_desName__",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "ICON 128.png",
      "16": "ICON 16.png",
      "32": "ICON 32.png",
      "64": "ICON 64.png"
   },
   "manifest_version": 3,
   "name": "__MSG_extName__",
   "permissions": [ "storage" ],
   "version": "3.2.7"
}
